{
	"name": "ventrata",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"prepare": "svelte-kit sync && npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@material/typography": "^13.0.0",
		"@mdi/js": "^6.7.96",
		"@smui/button": "^6.0.0-beta.16",
		"@smui/card": "^6.0.0-beta.16",
		"@smui/checkbox": "^6.0.0-beta.16",
		"@smui/data-table": "^6.0.0-beta.16",
		"@smui/dialog": "^6.0.0-beta.16",
		"@smui/form-field": "^6.0.0-beta.16",
		"@smui/layout-grid": "^6.0.0-beta.16",
		"@smui/radio": "^6.0.0-beta.16",
		"@smui/select": "^6.0.0-beta.16",
		"@smui/textfield": "^6.0.0-beta.16",
		"@smui/tooltip": "^6.0.0-beta.16",
		"@smui/top-app-bar": "^6.0.0-beta.16",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"smui-theme": "^6.0.0-beta.16",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-mui": "^0.5.0",
		"svelte-preprocess": "^4.10.1",
		"tslib": "^2.4.0",
		"typescript": "~4.6.2"
	},
	"type": "module",
	"dependencies": {
		"@material/button": "^14.0.0",
		"@sveltejs/adapter-node": "^1.0.0-next.78",
		"svelte-forms": "^2.3.1",
		"svelte-forms-lib": "^2.0.1",
		"svelte-simple-modal": "^1.3.4",
		"svelte-step-wizard": "^0.0.2",
		"svelte-steps": "^2.3.5",
		"yup": "^0.32.11",
		"zod": "^3.17.3"
	}
}
